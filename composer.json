{
    "name": "gitsccit/product-backend",
    "description": "ProductBackend plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "James Chen",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/authentication": "^2.0",
        "cakephp/cakephp": "^4.0",
        "gitsccit/web-skeleton": "dev-master",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "ProductBackend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProductBackend\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
